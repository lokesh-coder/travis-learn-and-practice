{
  "branch": "master",
  "dryRun": "false",
  "debug": "true",
  "verifyConditions": [
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/git"
  ],
  "prepare": [
    {
      "path": "@semantic-release/git",
      "assets": ["package.json"],
      "message":
        "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }
  ],
  "publish": ["@semantic-release/github"]
}
