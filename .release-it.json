{
  "non-interactive": true,
  "verbose": true,
  "increment": "conventional:angular",
  "beforeChangelogCommand":
    "conventional-changelog -p angular -i CHANGELOG.md -s",
  "changelogCommand": "conventional-changelog -p angular | tail -n +3",
  "preReleaseId": "beta",
  "requireCleanWorkingDir": false,
  "requireUpstream": false,
  "safeBump": false,
  "src": {
    "tagName": "v%s",
    "push": false,
    "afterReleaseCommand":
      "git push https://$GITHUB_TOKEN@github.com/lokesh-coder/travis-learn-and-practice.git HEAD:master",
    "commitMessage": "chore: version bumbed to v%s",
    "tagAnnotation": "v%s",
    "addUntrackedFiles": true
  },
  "github": {
    "release": true,
    "preRelease": true,
    "releaseName": "v%s"
  },
  "npm": {
    "publish": false
  }
}
